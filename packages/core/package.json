{
  "name": "@highduck/core",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "ci": "run build",
    "build": "tsc -b -f tsconfig.esm.json && run resources",
    "watch": "tsc -b -f -w tsconfig.esm.json",
    "resources": "cp src/graphics/util/2d.vert.glsl dist/graphics/util && cp src/graphics/util/2d.frag.glsl dist/graphics/util",
    "clean": "rimraf dist"
  },
  "sideEffects": false,
  "dependencies": {
    "@highduck/anijson": "0.0.1",
    "@highduck/math": "0.0.1",
    "@types/howler": "^2.2.0"
  },
  "devDependencies": {
    "howler": "^2.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  }
}
