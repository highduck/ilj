{
  "name": "@highduck/tools-build-code",
  "version": "0.0.1",
  "scripts": {
    "ci": "run build && run test-build",
    "build": "tsc -p tsconfig.esm.json",
    "watch": "tsc -w -p tsconfig.esm.json",
    "clean": "rimraf ./dist/ ./test/dist/",
    "test-build": "cd test && node --experimental-specifier-resolution=node test-build.js",
    "test-watch": "cd test && node --experimental-specifier-resolution=node test-watch.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.js",
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-virtual": "^2.0.3",
    "core-js": "^3.6.5",
    "glslify": "^7.0.0",
    "regenerator-runtime": "^0.13.7",
    "resolve": "^1.17.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.1",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-glslify": "^1.2.0",
    "rollup-plugin-postcss": "^3.1.4",
    "rollup-plugin-sourcemaps": "jkassis/rollup-plugin-sourcemaps#rollup-plugin-sourcemaps-v0.6.2-gitpkg",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "terser": "^5.0.0",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.9",
    "@types/node": "^14.0.27",
    "@types/resolve": "^1.17.1",
    "@types/rimraf": "^3.0.0"
  }
}
