{
  "name": "@highduck/tools-build-code",
  "version": "0.0.1",
  "scripts": {
    "ci": "run build && run test-build",
    "build": "tsc -b -f",
    "watch": "tsc -b -f -w",
    "clean": "rimraf ./dist/ ./test/dist/",
    "test-build": "cd test && node --experimental-specifier-resolution=node test-build.js",
    "test-watch": "cd test && node --experimental-specifier-resolution=node test-watch.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.js",
  "type": "module",
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-strip": "^1.3.3",
    "@rollup/plugin-virtual": "^2.0.2",
    "core-js": "^3.6.5",
    "glslify": "^7.0.0",
    "regenerator-runtime": "^0.13.5",
    "resolve": "^1.17.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.16.0",
    "rollup-plugin-analyzer": "^3.2.3",
    "rollup-plugin-glslify": "^1.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "terser": "^4.7.0",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@types/resolve": "^1.17.1",
    "@types/rimraf": "^3.0.0"
  }
}
