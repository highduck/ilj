{
  "name": "@highduck/binpack",
  "version": "0.0.1",
  "main": "dist/commonjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "ci": "run build && run test",
    "build": "tsc -b tsconfig.commonjs.json && tsc -b tsconfig.esm.json",
    "demo:build": "run build && cd demo && tsc && rollup --config rollup.config.js",
    "clean": "rimraf dist/ && rimraf demo/dist/ && rimraf test/dist/",
    "test": "cd test && tsc -b && node dist/commonjs/index.js"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.10.8",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.0.1",
    "typescript": "^3.9.3"
  }
}
