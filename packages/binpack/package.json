{
  "name": "@highduck/binpack",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "ci": "run build && run test",
    "build": "tsc -b tsconfig.esm.json",
    "demo:build": "run build && cd demo && tsc -b && rollup -c rollup.config.js && rollup -c rollup.legacy.js",
    "clean": "rimraf dist/ && rimraf demo/dist/ && rimraf test/dist/",
    "test": "cd test && tsc -b && node dist/index.js"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.16.0",
    "rollup-plugin-analyzer": "^3.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "typescript": "^3.9.5"
  }
}
