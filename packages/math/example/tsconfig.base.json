{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "outFile": "index.js",
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "noEmitOnError": true
  },
  "files": [
    "index.ts"
  ],
  "exclude": [
    "**/node_modules",
    "dist/**"
  ]
}