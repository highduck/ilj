{
  "name": "@highduck/tools-build-code",
  "version": "0.0.1",
  "scripts": {
    "ci": "run build && run test-build",
    "build": "tsc -b tsconfig.project.json",
    "watch": "tsc -b -w tsconfig.project.json",
    "clean": "rimraf ./dist/ ./test/dist/",
    "test": "jest",
    "test-build": "cd test && node --experimental-specifier-resolution=node test-build.js",
    "test-watch": "cd test && node --experimental-specifier-resolution=node test-watch.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.js",
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-virtual": "^2.0.3",
    "@types/jest": "^26.0.14",
    "core-js": "^3.6.5",
    "glslify": "^7.1.1",
    "jest": "^26.4.2",
    "regenerator-runtime": "^0.13.7",
    "resolve": "^1.17.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-external-globals": "^0.6.0",
    "rollup-plugin-glslify": "^1.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-sourcemaps": "jkassis/rollup-plugin-sourcemaps#rollup-plugin-sourcemaps-v0.6.2-gitpkg",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.1.1",
    "terser": "^5.3.3",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.10",
    "@types/node": "^14.11.2",
    "@types/resolve": "^1.17.1",
    "@types/rimraf": "^3.0.0"
  }
}
